{
  "name": "@fastify/jwt",
  "version": "9.0.2",
  "description": "JWT utils for Fastify",
  "main": "jwt.js",
  "type": "commonjs",
  "types": "types/jwt.d.ts",
  "scripts": {
    "lint": "standard",
    "lint:fix": "standard --fix",
    "test": "npm run lint && npm run test:unit && npm run test:typescript",
    "test:typescript": "tsd",
    "test:unit": "tap",
    "test:unit:report": "tap --coverage-report=html",
    "test:unit:verbose": "tap -Rspec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fastify/fastify-jwt.git"
  },
  "keywords": [
    "jwt",
    "json",
    "token",
    "jsonwebtoken",
    "fastify"
  ],
  "author": "Tomas Della Vedova - @delvedor (http://delved.org)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fastify/fastify-jwt/issues"
  },
  "homepage": "https://github.com/fastify/fastify-jwt#readme",
  "dependencies": {
    "@fastify/error": "^4.0.0",
    "@lukeed/ms": "^2.0.2",
    "fast-jwt": "^5.0.0",
    "fastify-plugin": "^5.0.0",
    "steed": "^1.1.3"
  },
  "devDependencies": {
    "@fastify/cookie": "^11.0.1",
    "@fastify/pre-commit": "^2.1.0",
    "@types/node": "^22.0.0",
    "fastify": "^5.0.0",
    "standard": "^17.1.0",
    "tap": "^18.7.1",
    "tsd": "^0.31.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
